@{
    ViewBag.Title = "自定义表单功能";
    Layout = "~/Views/Shared/_CustmerIndexPreview.cshtml";
}
<div class="sp-layout">
    <div class="sp-layout-center">
        <div class="sp-layout-wrap sp-layout-wrap-notitle">
            <div class="sp-layout-tool">
                <div class="sp-layout-tool-left">
                    <div class="sp-layout-tool-item">
                        <div id="datesearch"></div>
                    </div>
                    <div class="sp-layout-tool-item" id="multiple_condition_query_item">
                        
                    </div>
                </div>
                <div class="sp-layout-tool-right">
                    <div class=" btn-group btn-group-sm">
                        <a id="sp_refresh" class="btn btn-default"><i class="fa fa-refresh"></i></a>
                    </div>
                    <div class=" btn-group btn-group-sm" sp-authorize="yes">
                        <a id="sp_add" class="btn btn-default"><i class="fa fa-plus"></i>&nbsp;新增</a>
                        <a id="sp_edit" class="btn btn-default"><i class="fa fa-pencil-square-o"></i>&nbsp;编辑</a>
                        <a id="sp_delete" class="btn btn-default"><i class="fa fa-trash-o"></i>&nbsp;删除</a>
                    </div>
                </div>
            </div>

            <div class="sp-layout-body" id="gridtable"></div>
        </div>
    </div>
</div>
@Html.AppendJsFile("/Areas/LR_FormModule/Views/FormRelation/PreviewIndex.js")

