@{
    ViewBag.Title = "设置excel导入字段属性";
    Layout = "~/Views/Shared/_Form.cshtml";
}
    <div class="sp-form-wrap" id="form">
        <div class="col-xs-12 sp-form-item">
            <div class="sp-form-item-title">设置字段<font face="宋体">*</font></div>
            <input id="F_Name" type="text" class="form-control" readonly />
        </div>
        <div class="col-xs-12 sp-form-item">
            <div class="sp-form-item-title">唯一性<font face="宋体">*</font></div>
            <label><input id="F_OnlyOne_y" name="F_OnlyOne" value="1" type="radio">组成</label>
            &nbsp;
            <label><input id="F_OnlyOne_n" name="F_OnlyOne" value="0" type="radio">无</label>
        </div>
        <div class="col-xs-12 sp-form-item">
            <div class="sp-form-item-title">关联类型<font face="宋体">*</font></div>
            <div id="F_RelationType" isvalid="yes" checkexpession="NotNull"></div>
        </div>
        <div class="col-xs-12 sp-form-item">
            <div class="sp-form-item-title">数据选项</div>
            <div id="F_DataItemCode"></div>
        </div>
        <div class="col-xs-12 sp-form-item">
            <div class="sp-form-item-title">数据选项</div>
            <div id="F_DataSourceId"></div>
        </div>
        <div class="col-xs-12 sp-form-item">
            <div class="sp-form-item-title">固定数值</div>
            <input id="F_Value" type="text" class="form-control" />
        </div>
    </div>
@Html.AppendJsFile("/Areas/LR_SystemModule/Views/ExcelImport/SetFieldForm.js")